<?xml version="1.0" encoding="UTF-8"?>
<suite name="RegressionSuite" parallel="tests" thread-count="2">
    <test name = "RegressionFirefox">
        <parameter name ="driverSelected" value = "Firefox"/>
        <classes>
            <class name = "test.java.abhshoes.herokuapp.com.RegresioNegativeNewAdress"/>
            <class name ="test.java.abhshoes.herokuapp.com.RegresionNegativeRegistration"/>
            <class name = "test.java.abhshoes.herokuapp.com.RegresionNegativeSearch"/>
            <class name = "test.java.abhshoes.herokuapp.com.RegresionSimpleSearch"/>
            <class name = "test.java.abhshoes.herokuapp.com.RegresionNegativeNegativeBuyIn"/>
        </classes>
    </test>

    <test name = "RegressionChrome">
        <parameter name ="driverSelected" value = "Chrome"/>
        <classes>
            <class name = "test.java.abhshoes.herokuapp.com.RegresioNegativeNewAdress"/>
            <class name ="test.java.abhshoes.herokuapp.com.RegresionNegativeRegistration"/>
            <class name = "test.java.abhshoes.herokuapp.com.RegresionNegativeSearch"/>
            <class name = "test.java.abhshoes.herokuapp.com.RegresionSimpleSearch"/>
            <class name = "test.java.abhshoes.herokuapp.com.RegresionNegativeNegativeBuyIn"/>
        </classes>
    </test>
</suite>