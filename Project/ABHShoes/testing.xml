<?xml version="1.0" encoding="UTF-8"?>
<suite name="testing">
    <test name="Smoke">
        <parameter name = "driverSelected" value = "Firefox"/>
        <classes>
            <class name="abhshoes.herokuapp.com.SmokeTest"/>
        </classes>
    </test>
    <test name = "Regression">
        <parameter name ="driverSelected" value = "Firefox"/>
        <classes>
            <class name = "abhshoes.herokuapp.com.RegresioNegativeNewAdress"/>
            <class name ="abhshoes.herokuapp.com.RegresionNegativeRegistration"/>
            <class name = "abhshoes.herokuapp.com.RegresionNegativeSearch"/>
            <class name = "abhshoes.herokuapp.com.RegresionSimpleSearch"/>
            <class name = "abhshoes.herokuapp.com.RegresionNegativeNegativeBuyIn"/>
        </classes>
    </test>
</suite>